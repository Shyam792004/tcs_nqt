import java.util.*;

public class Main
{
	public static void main(String[] args)
	{
		Scanner s=new Scanner(System.in);
// 		int a=s.nextInt();
		int n=5;
		int arr[]=new int[n];
		for(int i=0;i<n;i++)
		{
		    arr[i]=s.nextInt();
		}
		int max=Integer.MIN_VALUE;
		for(int i=0;i<n;i++)
		{
		    if(max<arr[i])
		    {
		        max=arr[i];
		    }
		}
		System.out.print(max);
	}
}
