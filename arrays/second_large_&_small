import java.util.*;

public class Main
{
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++)
        {
          arr[i]=sc.nextInt();
        }
        int seclar=0;
        int secmin=0;
        Arrays.sort(arr);
        seclar=arr[n-2];
        secmin=arr[1];
        System.out.println(seclar);
        System.out.println(secmin);
	}
}
